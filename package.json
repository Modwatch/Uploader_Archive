{
  "name": "modwatch-uploader",
  "version": "0.0.2",
  "description": "CLI for Modwatch",
  "main": "dist/cli.js",
  "scripts": {
    "build": "node scripts/build.js",
    "build:scripts": "babel ./src/scripts --out-dir ./scripts",
    "watch": "node scripts/build.js -w ./src",
    "lint": "eslint ./src/",
    "dev": "npm run build && npm start",
    "test": "ava ./tests/*Spec.js ./tests/**/*Spec.js -v --require babel-register",
    "test:watch": "npm run test -- --watch",
    "prepublish": "npm run build:scripts && npm run build -- -m"
  },
  "preferGlobal": true,
  "bin": {
    "modwatch": "dist/cli.js"
  },
  "author": "Aaron Ballard",
  "repository": {
    "type": "git",
    "url": "https://github.com/Modwatch/Uploader.git"
  },
  "engines": {
    "node": ">=5.10.1"
  },
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-cli": "^6.7.7",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-register": "^6.7.2",
    "core-js": "^2.3.0",
    "eslint": "^2.9.0",
    "express": "^4.13.4",
    "ora": "^0.2.1",
    "rollup": "^0.25.8",
    "rollup-plugin-babel": "^2.4.0",
    "rollup-plugin-commonjs": "^2.2.1",
    "rollup-plugin-json": "^2.0.0",
    "rollup-plugin-node-resolve": "^1.5.0",
    "uglify-js": "^2.6.2",
    "watch": "^0.18.0"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "core-js": "^2.3.0",
    "denodeify": "^1.2.1",
    "keypress": "^0.2.1",
    "node-fetch": "^1.5.1"
  }
}
